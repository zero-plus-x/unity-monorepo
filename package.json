    "@auto/commit-prompt": "^0.2.0",
    "@auto/core": "^0.3.0",
    "@auto/github": "^0.2.0",
    "@auto/slack": "^0.2.0",
    "@auto/telegram": "^0.2.0",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@nextools/eslint-config": "^0.4.0",
    "@nextools/typescript-config": "^0.2.0",
    "@start/plugin": "^2.0.0",
    "@start/plugin-sequence": "^2.0.0",
    "@start/reporter-verbose": "^2.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/prompts": "^2.0.8",
    "@types/puppeteer-core": "^2.0.0",
    "@types/xml2js": "^0.4.5",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "execa": "^4.0.3",
    "figures": "^3.2.0",
    "globby": "^11.0.0",
    "node-fetch": "^2.6.0",
    "pifs": "^2.0.0",
    "prompts": "^2.3.2",
    "puppeteer-core": "^5.2.1",
    "tsfn": "^2.0.0",
    "typeon": "^2.0.0",
    "typescript": "^3.9.7",
    "xml2js": "^0.4.23"
  },
  "start": {
    "file": "tasks/"
  },
  "auto": {
    "npm": {
      "access": "public"
    },
    "prefixes": {
      "major": "💥",
      "minor": "🌱",
      "patch": "🐞",
      "publish": "📦",
      "dependencies": "♻️",
      "initial": "🐣",
      "test": "👾",
      "docs": "📝",
      "refactor": "🛠",
      "wip": "🚧"
    },
    "commit": {
      "major": "Breaking Change",
      "minor": "New feature",
      "patch": "Bugfix",
      "initial": "Initial",
      "depepdencies": "Dependencies",
      "test": "Test",
      "docs": "Docs",
      "refactor": "Refactor",
      "wip": "WIP"
    }
  },
  "eslintConfig": {
    "extends": "@nextools/eslint-config"
  },
  "eslintIgnore": [
    "build/",
    "coverage/",
    "node_modules/"
  ]
}
